<Window x:Class="GraphWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Graphing Data" Height="350" Width="525">
    <Grid>
        <Button Content="Get Data" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="GetDataButton" VerticalAlignment="Top" Width="75" Click="GetDataButton_Click" />
        <TreeView Height="290" HorizontalAlignment="Left" Margin="113,9,0,0" Name="DataDisplayTreeView" VerticalAlignment="Top" Width="380" OverridesDefaultStyle="False" Background="{x:Null}" FontSize="12" />
        <Button Content="Graph" Height="23" HorizontalAlignment="Left" Margin="12,45,0,0" Name="GenerateGraphButton" VerticalAlignment="Top" Width="75" Click="GenerateGraphButton_Click" />
    </Grid>
</Window>
